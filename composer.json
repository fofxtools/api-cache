{
    "name": "fofx/api-cache",
    "description": "Laravel-based API response caching library",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "FOfX\\ApiCache\\": "src/"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "FOfX\\ApiCache\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.3",
        "laravel/framework": "^12.22",
        "fofx/helper": "^1.2",
        "guzzlehttp/guzzle": "^7.9",
        "fofx/utility": "~0.0.2"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.85",
        "phpstan/phpstan": "^2.1",
        "phpunit/phpunit": "^12.3",
        "orchestra/testbench": "^10.4",
        "phpstan/phpstan-mockery": "^2.0",
        "phpdocumentor/graphviz": "^2.1",
        "phpdocumentor/reflection": "^6.3"
    },
    "scripts": {
        "phpstan": "vendor/bin/phpstan analyse",
        "analyse": "@phpstan",
        "analyze": "@phpstan",
        "phpunit": "vendor/bin/phpunit --color=always",
        "test": "@phpunit",
        "tests": "@phpunit",
        "cs-fix": "vendor/bin/php-cs-fixer fix",
        "cs-check": "vendor/bin/php-cs-fixer fix --dry-run --diff",
        "cs-list": "vendor/bin/php-cs-fixer list-files"
    },
    "extra": {
        "laravel": {
            "providers": [
                "FOfX\\ApiCache\\ApiCacheServiceProvider"
            ]
        }
    },
    "config": {
        "process-timeout": 0
    }
}
